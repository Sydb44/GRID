{"id": "selector.by_name.v1", "type": "exact_match", "pattern": "block_name", "description": "Select blocks by exact name match", "example": "Reactor 1", "validation": "must_exist", "case_sensitive": false}
{"id": "selector.by_partial_name.v1", "type": "partial_match", "pattern": "partial_name*", "description": "Select blocks by partial name match", "example": "Reactor*", "validation": "must_match_at_least_one", "wildcard_support": true}
{"id": "selector.by_group.v1", "type": "group_reference", "pattern": "group:group_name", "description": "Select all blocks in a user-defined group", "example": "group:Main Lights", "validation": "group_must_exist", "supports_nested": false}
{"id": "selector.by_type.v1", "type": "block_type", "pattern": "type:block_type", "description": "Select all blocks of specific type", "example": "type:Interior Light", "validation": "type_must_be_valid", "case_insensitive": true}
{"id": "selector.by_subtype.v1", "type": "block_subtype", "pattern": "subtype:subtype_id", "description": "Select blocks by technical subtype ID", "example": "subtype:LargeBlockBatteryBlock", "validation": "subtype_must_exist"}
{"id": "selector.by_zone.v1", "type": "spatial_area", "pattern": "zone:zone_name", "description": "Select blocks in named spatial area", "example": "zone:bridge", "validation": "zone_must_be_defined", "requires_coordinates": true}
{"id": "selector.nearest_to.v1", "type": "proximity", "pattern": "nearest:coordinate", "description": "Select closest block to specified location", "example": "nearest:GPS:Origin:0:0:0:", "validation": "coordinate_format", "max_distance": 50000}
{"id": "selector.within_range.v1", "type": "proximity", "pattern": "within:distance:coordinate", "description": "Select blocks within range of location", "example": "within:100m:GPS:Base:0:0:0:", "validation": "coordinate_and_distance", "units": ["m", "km"]}
{"id": "selector.on_grid.v1", "type": "grid_scope", "pattern": "grid:current|connected|all", "description": "Limit selection to specific grid scope", "example": "grid:connected", "validation": "valid_scope", "default": "current"}
{"id": "selector.on_subgrid.v1", "type": "grid_scope", "pattern": "subgrid:subgrid_name", "description": "Select blocks on specific subgrid", "example": "subgrid:Drilling Platform", "validation": "subgrid_exists"}
{"id": "selector.all.v1", "type": "universal", "pattern": "all", "description": "Select all blocks of command's applicable type", "example": "all lights", "validation": "type_compatible", "safety_check": true}
{"id": "selector.powered.v1", "type": "state_filter", "pattern": "powered", "description": "Filter to only powered/functional blocks", "example": "powered reactors", "validation": "block_state"}
{"id": "selector.unpowered.v1", "type": "state_filter", "pattern": "unpowered", "description": "Filter to only unpowered/non-functional blocks", "example": "unpowered lights", "validation": "block_state"}
{"id": "selector.enabled.v1", "type": "state_filter", "pattern": "enabled", "description": "Filter to only enabled blocks", "example": "enabled batteries", "validation": "enabled_state"}
{"id": "selector.disabled.v1", "type": "state_filter", "pattern": "disabled", "description": "Filter to only disabled blocks", "example": "disabled sensors", "validation": "enabled_state"}
{"id": "selector.damaged.v1", "type": "state_filter", "pattern": "damaged", "description": "Filter to blocks below functional threshold", "example": "damaged armor", "validation": "integrity_check"}
{"id": "selector.functional.v1", "type": "state_filter", "pattern": "functional", "description": "Filter to fully functional blocks", "example": "functional thrusters", "validation": "integrity_check"}
{"id": "selector.with_inventory.v1", "type": "capability_filter", "pattern": "with_inventory", "description": "Filter to blocks that have inventory", "example": "containers with_inventory", "validation": "has_inventory"}
{"id": "selector.connected_to.v1", "type": "connection_filter", "pattern": "connected_to:block_name", "description": "Select blocks connected via conveyors", "example": "connected_to:Main Cargo", "validation": "conveyor_path_exists"}
{"id": "selector.owned_by.v1", "type": "ownership_filter", "pattern": "owned_by:player_name", "description": "Filter by block ownership", "example": "owned_by:PlayerName", "validation": "player_exists", "multiplayer_only": true}
{"id": "selector.faction.v1", "type": "ownership_filter", "pattern": "faction:faction_name", "description": "Filter by faction ownership", "example": "faction:Mining Corp", "validation": "faction_exists", "multiplayer_only": true}
{"id": "selector.above.v1", "type": "spatial_relative", "pattern": "above:reference", "description": "Select blocks above reference point", "example": "above:GPS:Base:0:0:0:", "validation": "coordinate_format"}
{"id": "selector.below.v1", "type": "spatial_relative", "pattern": "below:reference", "description": "Select blocks below reference point", "example": "below:current_position", "validation": "coordinate_format"}
{"id": "selector.north_of.v1", "type": "spatial_relative", "pattern": "north_of:reference", "description": "Select blocks north of reference", "example": "north_of:GPS:Base:0:0:0:", "validation": "coordinate_format"}
{"id": "selector.south_of.v1", "type": "spatial_relative", "pattern": "south_of:reference", "description": "Select blocks south of reference", "example": "south_of:GPS:Base:0:0:0:", "validation": "coordinate_format"}
{"id": "selector.east_of.v1", "type": "spatial_relative", "pattern": "east_of:reference", "description": "Select blocks east of reference", "example": "east_of:current_position", "validation": "coordinate_format"}
{"id": "selector.west_of.v1", "type": "spatial_relative", "pattern": "west_of:reference", "description": "Select blocks west of reference", "example": "west_of:GPS:Base:0:0:0:", "validation": "coordinate_format"}
{"id": "selector.first.v1", "type": "ordering", "pattern": "first:N", "description": "Select first N blocks from selection", "example": "first:5 batteries", "validation": "positive_integer", "default_n": 1}
{"id": "selector.last.v1", "type": "ordering", "pattern": "last:N", "description": "Select last N blocks from selection", "example": "last:3 thrusters", "validation": "positive_integer", "default_n": 1}
{"id": "selector.random.v1", "type": "ordering", "pattern": "random:N", "description": "Select N random blocks from selection", "example": "random:2 lights", "validation": "positive_integer", "default_n": 1}
{"id": "selector.exclude.v1", "type": "exclusion", "pattern": "exclude:selector", "description": "Exclude blocks matching selector", "example": "all lights exclude:disabled", "validation": "valid_sub_selector"}
{"id": "selector.only.v1", "type": "inclusion", "pattern": "only:selector", "description": "Include only blocks matching selector", "example": "batteries only:enabled", "validation": "valid_sub_selector"}
{"id": "selector.and.v1", "type": "logical", "pattern": "selector1 and selector2", "description": "Logical AND combination of selectors", "example": "powered and enabled", "validation": "valid_sub_selectors"}
{"id": "selector.or.v1", "type": "logical", "pattern": "selector1 or selector2", "description": "Logical OR combination of selectors", "example": "damaged or disabled", "validation": "valid_sub_selectors"}
{"id": "selector.not.v1", "type": "logical", "pattern": "not selector", "description": "Logical NOT of selector", "example": "not damaged", "validation": "valid_sub_selector"}
{"id": "selector.empty.v1", "type": "inventory_state", "pattern": "empty", "description": "Filter to blocks with empty inventory", "example": "empty containers", "validation": "has_inventory"}
{"id": "selector.full.v1", "type": "inventory_state", "pattern": "full", "description": "Filter to blocks with full inventory", "example": "full cargo", "validation": "has_inventory"}
{"id": "selector.containing.v1", "type": "inventory_content", "pattern": "containing:item_name", "description": "Filter to blocks containing specific item", "example": "containing:Iron Ore", "validation": "valid_item_name"}
{"id": "selector.missing.v1", "type": "inventory_content", "pattern": "missing:item_name", "description": "Filter to blocks missing specific item", "example": "missing:Uranium Ingot", "validation": "valid_item_name"}
{"id": "selector.priority.v1", "type": "ordering", "pattern": "priority:high|medium|low", "description": "Select by operational priority", "example": "priority:high reactors", "validation": "valid_priority"}
{"id": "selector.recently_used.v1", "type": "temporal", "pattern": "recently_used:time_period", "description": "Filter to recently used blocks", "example": "recently_used:5m", "validation": "valid_time_format", "units": ["s", "m", "h"]}
{"id": "selector.idle.v1", "type": "state_filter", "pattern": "idle", "description": "Filter to idle/unused blocks", "example": "idle assemblers", "validation": "operational_state"}
{"id": "selector.active.v1", "type": "state_filter", "pattern": "active", "description": "Filter to active/working blocks", "example": "active drills", "validation": "operational_state"}
{"id": "selector.overloaded.v1", "type": "state_filter", "pattern": "overloaded", "description": "Filter to overloaded power consumers", "example": "overloaded thrusters", "validation": "power_state"}
{"id": "selector.current.v1", "type": "context", "pattern": "current", "description": "Reference to currently controlled block/ship", "example": "current ship", "validation": "has_control_context"}
{"id": "selector.this.v1", "type": "context", "pattern": "this", "description": "Reference to the executing block itself", "example": "this programmable block", "validation": "execution_context"}